{
  "name": "open-trainer",
  "version": "0.0.1",
  "description": "OpenTrainer helps people learn and retain information on any topic. It is based on the concept of flash cards.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint src test",
    "fixlint": "eslint src test --fix",
    "test": "jest --coverage ./test/unit",
    "integration": "cross-env NODE_ENV=user jest --coverage ./test/integration",
    "verify": "npm run fixlint && npm run lint && npm run test && npm run integration",
    "TUI": "cross-env NODE_ENV=user node src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brian1568/open-trainer.git"
  },
  "keywords": [
    "universal",
    "trainer",
    "learn",
    "retain"
  ],
  "author": "Brian C. Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brian1568/open-trainer/issues"
  },
  "homepage": "https://github.com/brian1568/open-trainer#readme",
  "devDependencies": {
    "chance": "^1.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.3.1"
  },
  "dependencies": {
    "config": "^3.3.6"
  }
